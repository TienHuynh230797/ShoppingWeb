<head>
    <title>{{title}}</title>
</head>
<body>
<div class="header">
    <div class="container">
        <div class="header-bottom-bottom">
            <div class="top-nav">
                <span class="menu"> </span>
                <ul>
                    <li><a href="/">Home</a></li>
                    {{#each type_list}}
                        {{#ifEquals this._id ../typeCurrent._id}}
                            <li class="active"><a href="/type/{{this._id}}">{{this.name}}</a></li><!--men.html-->
                        {{else}}
                            <li><a href="/type/{{this._id}}">{{this.name}}</a></li><!--men.html-->
                        {{/ifEquals}}
                    {{/each}}
                </ul>
                <script>
                    $("span.menu").click(function () {
                        $(".top-nav ul").slideToggle(500, function () {
                        });
                    });
                </script>
                <div class="clearfix"></div>
            </div>
            <div class="search">
                <form>
                    <input type="text" value="Search" onfocus="this.value = '';"
                           onblur="if (this.value === '') {this.value = '';}">
                    <input type="submit" value="">
                </form>
            </div>
            <div class="clearfix"></div>
        </div>
    </div>
</div>
<div class="content">
    <div class="container">
        <div class="women-in">
            <div class="col-md-9 col-d">
                <div class="in-line">
                    <div class="lady-in">
                        {{# each product_list}}
                            <div class="col-md-4 you-para">
                                <a href="{{this.url}}"><img class="img-responsive pic-in" src="../{{this.image}}"
                                                            alt="ERROR"></a>
                                {{#if this.discount_amount}}
                                    <div class="you-in">
                                        <span><label>{{this.discount_amount}}%</label></span>
                                        <small>off</small>
                                    </div>
                                {{/if}}
                                <p>{{this.product_name}}</p>
                                <span>${{this.price}}</span> | <label class="cat-in"> </label> <a href="#">Add to
                                Cart </a>
                            </div>
                        {{/each}}
                    </div>
                </div>
            </div>
            <div class="col-md-3 col-m-left">
                <div class="in-left">
                    <p class="code">Cool COLLECTIONS</p>
                    <div class="cool">
                    </div>
                </div>
                <div class="discount">
                    <a href="#"><img class="img-responsive pic-in" src="../images/p2.jpg" alt=" "></a><!--single.html-->
                    <p class="no-more">Exclusive <b>discount</b> <span>Womens wear</span></p>
                    <a href="#" class="know-more">know more</a>
                </div>
                <div class="discount">
                    <a href="#"><img class="img-responsive pic-in" src="../images/p3.jpg" alt=" "></a><!--single.html-->
                    <p class="no-more no-get">Get Exclusive <b>discount on</b> <span>Ladies wear</span></p>
                    <a href="#" class="know-more">know more</a>
                </div>
                <div class="twitter-in">
                    <h5>TWITTER UPDATES</h5>
                    <span class="twitter-ic">	</span>
                    <div class="up-date">
                        <p>@suniljoshi Looks like nice and dicent design</p>
                        <a href="#">http://bit.ly/sun</a>
                        <p class="ago">About 1 hour ago via twitterfeed</p>
                    </div>
                    <div class="up-date">
                        <p>@suniljoshi Looks like nice and dicent design</p>
                        <a href="#">http://bit.ly/sun</a>
                        <p class="ago">About 1 hour ago via twitterfeed</p>
                    </div>
                    <a href="#" class="tweets">More Tweets</a>
                    <div class="clearfix"></div>
                </div>
            </div>
            <div class="clearfix"></div>
        </div>
    </div>
</div>
</body>