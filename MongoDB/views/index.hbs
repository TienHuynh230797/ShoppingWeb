<!--header-->
<div class="header">
    <div class="container">
        <div class="header-bottom-bottom">
            <div class="top-nav">
                <span class="menu"> </span>
                <ul>
                    <li class="active"><a href="/">Home</a></li>
                    {{# each type_list}}
                        <li><a href="type/{{this._id}}">{{this.name}}</a></li><!--men.html-->
                    {{/each}}
                </ul>
                <script>
                    $("span.menu").click(function () {
                        $(".top-nav ul").slideToggle(500, function () {
                        });
                    });
                </script>
                <div class="clearfix"></div>
            </div>
            <div class="search">
                <form action="/search/{{this.content}}" method="get">
                    <input type="text" name="content" placeholder="Search" onfocus="this.value = '';">
                    <input type="submit" value="">
                </form>
            </div>
            <div class="clearfix"></div>
        </div>
    </div>
</div>
<!--content-->
<div class="content">
    <div class="container">
        <div class="women-in">
            <div class="col-md-9 col-d">
                <div class="banner">
                    <div class="banner-matter">
                        <label>Collection</label>
                        <h2>Summmer</h2>
                        <p>Helping you look cool</p>

                    </div>
                    <div class="you">
                        <span>40<label>%</label></span>
                        <small>off</small>
                    </div>
                    <p class="para-in">Some text regarding the featured product.</p>
                </div>
                <!---->
                <div class="in-line">
                    <div class="para-an">
                        <h3>SOME PRODUCTS</h3>
                        <p>Check our all latest products in this section.</p>
                    </div>
                    <div class="lady-in">
                        {{# each product_list}}
                            <div class="col-md-4 you-para">
                                <a href="{{this.url}}"><img class="img-responsive pic-in"
                                                            src={{this.image}}></a>
                                {{#if this.discount}}
                                    <div class="you-in">
                                        <span><label>{{this.discount}}%</label></span>
                                        <small>off</small>
                                    </div>
                                {{/if}}
                                <p>{{this.product_name}}</p>
                                <span>{{formatCurrency this.discount_price}}â‚«</span> | <label class="cat-in"> </label>
                                <!--<a href="/this.url>Add to Cart </a>-->
                                <a href="/add-to-cart/{{this._id}}">Add to Cart </a>
                            </div>
                        {{/each}}
                    </div>
                </div>
            </div>
            <div class="col-md-3 col-m-left">
                <div class="discount">
                    <a href="#"><img class="img-responsive" src="images/p2.jpg" alt=" "></a><!--single.html-->
                    <p class="no-more">Exclusive <b>discount</b> <span>Womens wear</span></p>
                    <a href="#" class="know-more">know more</a>
                </div>
                <div class="discount">
                    <a href="#"><img class="img-responsive" src="images/p3.jpg" alt=" "></a><!--single.html-->
                    <p class="no-more no-get">Get Exclusive <b>discount on</b> <span>Ladies wear</span></p>
                    <a href="#" class="know-more">know more</a>
                </div>
                <div class="twitter-in">
                    <h5>TWITTER UPDATES</h5>
                    <span class="twitter-ic">	</span>
                    <div class="up-date">
                        <p>@suniljoshi Looks like nice and dicent design</p>
                        <a href="#">http://bit.ly/sun</a>
                        <p class="ago">About 1 hour ago via twitterfeed</p>
                    </div>
                    <div class="up-date">
                        <p>@suniljoshi Looks like nice and dicent design</p>
                        <a href="#">http://bit.ly/sun</a>
                        <p class="ago">About 1 hour ago via twitterfeed</p>
                    </div>
                    <a href="#" class="tweets">More Tweets</a>
                    <div class="clearfix"></div>
                </div>
            </div>
            <div class="clearfix"></div>
        </div>
